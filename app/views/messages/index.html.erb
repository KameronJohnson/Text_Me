<div class="col-md-7 col-md-offset-2">
  <div class="cool-border">
  <h2>Messages</h2>

    <ul id="messages-list">
      <% @messages.each do |message| %>
        <li><%= link_to message.body, message_path(message) %></li>
    <% end %>
    </ul>
    <%= link_to "New Message", new_message_path, id: "new_link", remote: true %> |
    <%= link_to "Contacts", contacts_path %>


  </div>
</div>
