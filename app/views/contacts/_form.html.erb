<%= render "layouts/errors", :object => @contact %>

<%= form_for @contact do |form| %>
  <div class = "form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: 'form-control' %>
  </div>
  <div class = "form-group">
    <%= form.label :phone %>
    <%= form.text_field :phone, class: 'form-control' %>
  </div>
  <%= form.submit class: 'btn btn-md btn-danger' %>
<% end %>
